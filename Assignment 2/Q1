#include <iostream>
#include <algorithm>
using namespace std;

struct Activity {
    int start, finish;
};

bool compare(Activity a, Activity b) {
    return a.finish < b.finish;
}

int main() {
    int n = 6;
    Activity act[] = {{1,2},{3,4},{0,6},{5,7},{8,9},{5,9}};

    sort(act, act + n, compare);

    int count = 1;
    int lastFinish = act[0].finish;

    cout << "Selected activities:\n";
    cout << "(" << act[0].start << "," << act[0].finish << ")\n";

    for(int i = 1; i < n; i++) {
        if(act[i].start >= lastFinish) {
            cout << "(" << act[i].start << "," << act[i].finish << ")\n";
            lastFinish = act[i].finish;
            count++;
        }
    }

    cout << "Maximum number of activities = " << count;
    return 0;
}
